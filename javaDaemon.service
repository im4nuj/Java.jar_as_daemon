[Unit]
Description = RestAPI for Daemon Service
After = syslog.target network.target

[Service]
Working Directory=/home/ec2-user/daemon/target/
ExecStart=/usr/lib/jvm/java-1.8.0-openjdk-1.8.0.342.b07-1.amzn2.0.1.x86_64/jre/bin/java -jar daemonDemo-0.0.1-SNAPSHOT.jar
ExecStop=/bin/kill -15 $MAINPID

[Install]
WantedBy=multi-user.target